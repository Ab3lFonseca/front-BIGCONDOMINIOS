{
  "builds": [
    {
      "src": "src/pages/**",
      "use": "@vercel/static"
    },
    {
      "src": "src/controllers/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/styles/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "src/pages/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "src/pages/$1"
    }
  ]
}
